    FORMAT HDF5
    PERIODIC TIME 100 y
    VARIABLES
      TEMPERATURE
      LIQUID_PRESSURE
      LIQUID_DENSITY
      LIQUID_SATURATION
      LIQUID_VISCOSITY
      PERMEABILITY
      POROSITY
    /
  /
  VELOCITY_AT_CENTER
END



REGION all
  COORDINATES
    0.d0 0.d0 0.d0
    1500 1.d2 1500
  /
END

REGION downwelling
  COORDINATES
    175  0.d0 1090
    1500 1.d2 1500
  /
END

REGION upwelling
  COORDINATES
    0    0.d0 1090
    60   1.d2 1500
  /
END


REGION low_perm
  COORDINATES
    60   0.d0 1090
    175  1.d2 1500
  /
END


REGION reaction_zone
  COORDINATES
    0.d0  0.d0  0.d0
    1500  1.d2  1.d0
  /
  FACE BOTTOM
END


REGION seafloor
  COORDINATES
    0.00d0 0.d0  1499
    1500   1.d2  1500
  /
  FACE TOP
END


FLOW_CONDITION initial
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
    TEMPERATURE DIRICHLET
  /
  DATUM 0 0.d0 1500.d0
  LIQUID_PRESSURE 3.d7
  TEMPERATURE 2
  GRADIENT
     TEMPERATURE -6.67d-4 0 -0.2
  /
END

FLOW_CONDITION seafloor
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
    TEMPERATURE DIRICHLET
  /
  DATUM 0.d0 0.d0 1500.d0
  LIQUID_PRESSURE 3.d7
  TEMPERATURE 2.d0
END

FLOW_CONDITION magma_lens
  TYPE
    LIQUID_FLUX NEUMANN
    TEMPERATURE DIRICHLET
  /
  TEMPERATURE 350.d0
  LIQUID_FLUX 0.d0
END


INITIAL_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION all
END

BOUNDARY_CONDITION
  FLOW_CONDITION seafloor
  TRANSPORT_CONDITION seafloor
  REGION seafloor
END

BOUNDARY_CONDITION
  FLOW_CONDITION magma_lens
  TRANSPORT_CONDITION empty
  REGION reaction_zone
END


STRATA
  REGION all
  MATERIAL sheeted_dykes
END

STRATA

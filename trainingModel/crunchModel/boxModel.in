!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!!Physics and geometry
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

DISCRETIZATION
xzones                       1 1
END
 
INITIAL_CONDITIONS
Seawater                     1-1
END

BOUNDARY_CONDITIONS
X_begin   Seawater     Dirichlet
X_end     Seawater     Dirichlet
END
 
TEMPERATURE
set_temperature              25.0 
END
 
!TRANSPORT
!distance_units centimeters
!time_units  seconds
!calculate_diffusion 0.919e-05
!END
 
!FLOW
!distance_units     meters
!time_units         years
!constant_flow      0.000015
!END

POROSITY
minimum_porosity    10E-14
porosity_update     true
END

!EROSION/BURIAL
!time_units                  years
!distance_units              meters
!erode_x                     0.00015
!END